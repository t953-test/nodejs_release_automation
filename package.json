{
  "name": "nodejs-release-automation",
  "version": "1.0.2",
  "description": "Testing Node.js release automation",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "files": [
    "cjs",
    "mjs",
    "types"
  ],
  "scripts": {
    "build:cjs": "tsc -p ./tsconfig.cjs.json && echo '{\"type\":\"commonjs\"}' > cjs/package.json",
    "build:mjs": "tsc -p ./tsconfig.mjs.json && echo '{\"type\":\"module\"}' > mjs/package.json",
    "build:types": "tsc -p ./tsconfig.cjs.json --declaration --emitDeclarationOnly --declarationDir './types'",
    "build": "npm run build:cjs & npm run build:mjs & npm run build:types",
    "prepare": "npm run build",
    "clean": "rm -rf cjs mjs types"
  },
  "author": {
    "name": "t953-test",
    "email": "t953githubactions@gmail.com",
    "url": "https://github.com/t953-test/nodejs_release_automation"
  },
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/node16": "^16.1.0",
    "@tsconfig/node18": "^18.2.0",
    "@types/jest": "^29.5.3",
    "@types/json-server": "^0.14.4",
    "@types/node": "^18.16.19",
    "@types/tmp": "^0.2.6",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "axios-mock-adapter": "^1.22.0",
    "eslint": "^8.46.0",
    "jest": "^29.6.1",
    "json-server": "^0.17.3",
    "prettier": "^3.0.0",
    "tmp": "^0.2.1",
    "ts-jest": "^29.1.1",
    "typescript": "~5.1.6"
  },
  "volta": {
    "node": "18.17.0"
  },
  "dependencies": {
    "axios": "^1.4.0"
  },
  "exports": {
    "types": "./types/index.d.ts",
    "import": "./mjs/index.js",
    "require": "./cjs/index.js"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "moduleNameMapper": {
      "ipaddr.js": "ipaddr.js",
      "^(.+)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./tsconfig.json"
      }
    },
    "testMatch": [
      "**/tests/**/*.test.+(ts|js)"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/t953-test/nodejs_release_automation.git"
  },
  "homepage": "https://github.com/t953-test/nodejs_release_automation"
}
